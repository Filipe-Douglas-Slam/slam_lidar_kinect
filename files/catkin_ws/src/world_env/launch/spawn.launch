<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <include file="$(find world_env)/launch/spawn_sdf.launch">
        <arg name="robot_name" value="env" />
        <arg name="x" value="0.0" />
        <arg name="y" value="0.0" />
        <arg name="z" value="0.0" />
        <arg name="roll" value="0"/>
        <arg name="pitch" value="0"/>
        <arg name="yaw" value="0.0" />
        <arg name="sdf_robot_file" value="$(find world_env)/models/ambienteSimples/model.sdf" />
    </include>
</launch>
